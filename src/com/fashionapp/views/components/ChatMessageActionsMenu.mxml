<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark"
		 height="35">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<s:Rect radiusX="3" radiusY="3"
			height="100%" width="100%">
		<s:fill>
			<s:SolidColor color="0x000000"/>
		</s:fill>
	</s:Rect>
	<s:HGroup height="100%" horizontalAlign="center" verticalAlign="middle">
		<s:Label paddingBottom="5" paddingLeft="5" paddingRight="5" paddingTop="5" fontSize="11" color="0xFFFFFF" text="Copy" height="100%"
				 textAlign="center"
				 verticalAlign="middle"/>
		<s:BorderContainer height="100%" width="1" borderVisible="false" backgroundColor="0xFFFFFF"/>
		<s:Label paddingBottom="5" paddingLeft="5" paddingRight="5" paddingTop="5" fontSize="11" color="0xFFFFFF" text="Delete" height="100%"
				 textAlign="center"
				 verticalAlign="middle"/>
		<s:BorderContainer height="100%" width="1" borderVisible="false" backgroundColor="0xFFFFFF"/>
		<s:Label paddingBottom="5" paddingLeft="5" paddingRight="5" paddingTop="5" fontSize="11" color="0xFFFFFF" text="Forward" height="100%"
				 textAlign="center"
				 verticalAlign="middle"/>
	</s:HGroup>
</s:Group>
